{
  "dashboard": {
    "id": null,
    "title": "AgentHub Agent Monitoring",
    "tags": ["agenthub", "agents", "containers"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Agent Container CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{agent_type=~\".*\"}[5m]) * 100",
            "legendFormat": "{{deployment_id}} ({{agent_type}})"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Usage %",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Agent Container Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{agent_type=~\".*\"} / 1024 / 1024",
            "legendFormat": "{{deployment_id}} ({{agent_type}})"
          }
        ],
        "yAxes": [
          {
            "label": "Memory Usage (MB)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Agent Container Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total{agent_type=~\".*\"}[5m]) / 1024",
            "legendFormat": "{{deployment_id}} - Receive (KB/s)"
          },
          {
            "expr": "rate(container_network_transmit_bytes_total{agent_type=~\".*\"}[5m]) / 1024",
            "legendFormat": "{{deployment_id}} - Transmit (KB/s)"
          }
        ],
        "yAxes": [
          {
            "label": "Network I/O (KB/s)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Agent Container Disk I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_fs_reads_total{agent_type=~\".*\"}[5m])",
            "legendFormat": "{{deployment_id}} - Reads (ops/s)"
          },
          {
            "expr": "rate(container_fs_writes_total{agent_type=~\".*\"}[5m])",
            "legendFormat": "{{deployment_id}} - Writes (ops/s)"
          }
        ],
        "yAxes": [
          {
            "label": "Disk I/O (ops/s)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Active Agent Containers by Type",
        "type": "stat",
        "targets": [
          {
            "expr": "count(container_cpu_usage_seconds_total{agent_type=\"function\"})",
            "legendFormat": "Function Agents"
          },
          {
            "expr": "count(container_cpu_usage_seconds_total{agent_type=\"acp\"})",
            "legendFormat": "ACP Agents"
          },
          {
            "expr": "count(container_cpu_usage_seconds_total{agent_type=\"persistent\"})",
            "legendFormat": "Persistent Agents"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Agent Container Status",
        "type": "table",
        "targets": [
          {
            "expr": "container_cpu_usage_seconds_total{agent_type=~\".*\"}",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "Value": true
              },
              "renameByName": {
                "agent_type": "Type",
                "deployment_id": "Deployment ID",
                "agent_id": "Agent ID",
                "hiring_id": "Hiring ID",
                "user_id": "User ID"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
