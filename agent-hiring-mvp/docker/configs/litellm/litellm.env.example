# LiteLLM Configuration
# Copy this file to .env and fill in your actual values

# LiteLLM Master Key - Users need this to access your proxy
LITELLM_MASTER_KEY=your-liteLLM-master-key-here

# Provider API Keys (for backend LLM access)
# Azure OpenAI
AZURE_API_KEY=your-azure-openai-key-here
AZURE_API_BASE=https://your-resource.openai.azure.com/

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic (Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Google (Gemini)
GOOGLE_API_KEY=your-google-api-key-here

# Cohere
COHERE_API_KEY=your-cohere-api-key-here

# Hugging Face
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# Replicate
REPLICATE_API_KEY=your-replicate-api-key-here

# Together AI
TOGETHER_API_KEY=your-together-api-key-here

# Groq
GROQ_API_KEY=your-groq-api-key-here
